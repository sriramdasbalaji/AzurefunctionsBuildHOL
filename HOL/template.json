{
    "$schema": "http://schema.management.azure.com/schemas/2014-04-01-preview/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "type": "String",
            "defaultValue": "Central US"
        },
        "projectName": {
            "type": "String",
            "defaultValue": "PartsUnlimited"
        },
        "processTemplateId": {
            "type": "String",
            "defaultValue": "ADCC42AB-9882-485E-A3ED-7678F01F66BC"
        },
        "processTemplateName": {
            "type": "String",
            "defaultValue": "Git"
        },
        "versionControlName": {
            "type": "String",
            "defaultValue": "Git"
        },
        "versionControlOption": {
            "type": "String",
            "defaultValue": "Git"
        },
        "ownerUpn": {
            "type": "String",
            "defaultValue": ""
        }
    },
    "variables": {
        "acctNameVar": "[concat('BuildWorkshop-',uniqueString(resourceGroup().id))]"
    },
    "resources": [
        {
            "type": "microsoft.visualstudio/account",
            "name": "[concat('BuildWorkshop-',uniqueString(resourceGroup().id))]",
            "apiVersion": "2014-04-01-preview",
            "location": "[parameters('location')]",
            "properties": {
                "operationType": "Create",
                "accountName": "[concat('BuildWorkshop-',uniqueString(resourceGroup().id))]",
                "ownerUpn": "[parameters('ownerUpn')]"
            },
            "resources": [
                {
                    "type": "project",
                    "name": "[parameters('projectName')]",
                    "apiVersion": "2014-04-01-preview",
                    "location": "[parameters('location')]",
                    "properties": {
                        "ProcessTemplateId": "[parameters('processTemplateId')]",
                        "VersionControlOption": "[parameters('versionControlOption')]",
                        "ownerUpn": "[parameters('ownerUpn')]"
                    },
                    "dependsOn": [
                        "[concat('microsoft.visualstudio/account/', variables('acctNameVar'))]"
                    ]
                }
            ]
        }
    ],
    "outputs": {}
}
